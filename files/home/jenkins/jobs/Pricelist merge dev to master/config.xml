  <builders>
    <hudson.tasks.Shell>
      <command>#!/bin/bash

# Check for empty commit id.
if [[ -z &quot;${COMMIT_ID}&quot; ]] ; then
    echo &apos;No commit id given.&apos;
    exit 1
fi

# Deploy Pricelist repo.
if [[ -d &quot;${WORKSPACE}/pricelist/.git&quot; ]] ; then
	echo &apos;Updating repo...&apos;
	cd &quot;${WORKSPACE}/pricelist&quot;
        for BRANCH in master dev ; do
            git checkout &quot;${BRANCH}&quot; || exit 1
            git pull origin &quot;${BRANCH}&quot; || exit 1
        done
else
	echo &apos;Deploying repo...&apos;
	cd &quot;${WORKSPACE}&quot;
	git clone git@github.com:ePages-de/pricelist.git
	cd &quot;${WORKSPACE}/pricelist&quot;
	git checkout dev
fi

# Does the commit id we have been given even exist?
git reset --hard &quot;${COMMIT_ID}&quot;
if [[ &quot;$?&quot; -ne 0 ]] ; then
	echo &apos;Commit id given seems to be a fake.&apos;
	exit 1
fi

# Merging promoted code into master.
echo &apos;Merging promoted version into master...&apos;
git checkout master ||Â exit 1
git merge dev
if [[ &quot;$?&quot; -ne 0 ]] ; then
    echo &apos;Merging dev into master failed...&apos;
    git reset --hard HEAD
    exit 1
fi

# Now we can push this state of master back into our origin.
echo &apos;Will push master back into origin now...&apos;
git push origin master</command>
    </hudson.tasks.Shell>
