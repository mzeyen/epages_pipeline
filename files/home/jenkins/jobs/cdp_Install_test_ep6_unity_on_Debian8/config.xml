  <builders>
    <hudson.tasks.Shell>
      <command>#!/bin/bash
echo &quot;SNIPPET 1: epages-infra&quot;

if [[ ! -d ~/epages-infra/.git ]] ; then
    cd ~
    git clone git@github.com:ePages-de/epages-infra.git
else
    cd ~/epages-infra &amp;&amp; git pull origin master
fi
</command>
    </hudson.tasks.Shell>
    <hudson.tasks.Shell>
      <command>#!/bin/bash
echo &quot;SNIPPET 2: jenkins-installs-epages.sh&quot;

export BUILD_ID=dontKillMe
~/epages-infra/scripts/epages/jenkins-installs-epages.sh -i simple/install-epages-unity.conf -r ftp://ftp.epages.com/pub/epages/build/repo/$VERSION/latest</command>
    </hudson.tasks.Shell>
    <hudson.tasks.Shell>
      <command>#!/bin/bash
echo &quot;SNIPPET 3: create shops&quot;

export BUILD_ID=dontKillMe
. /etc/default/epages6


CREATE_SHOP=/srv/epages/eproot/Cartridges/DE_EPAGES/ShopConfiguration/Scripts/createShop.pl
for shop in testshopunity esf-test ; do
  echo $PERL $CREATE_SHOP -wsuser Providers/Distributor/Users/admin -passwd admin -shoptype UnityDemo -storename Store -alias $shop -scriptname $shop
  $PERL $CREATE_SHOP -wsuser Providers/Distributor/Users/admin -passwd admin -shoptype UnityDemo -storename Store -alias $shop -scriptname $shop || exit 1
done
</command>
    </hudson.tasks.Shell>
    <hudson.tasks.Shell>
      <command>#!/bin/bash
echo &quot;SNIPPET 4: create_fingerprint.sh&quot;

export BUILD_ID=dontKillMe
export EXCLUDE_FILES_IN_NORMALIZE=&apos;mysql_config.txt DBSchema/GlobalVariables XML/ShopTypes&apos;
~/epages-infra/scripts/epages/fingerprint/create_fingerprint.sh -a</command>
    </hudson.tasks.Shell>
    <hudson.tasks.Shell>
      <command>#!/bin/bash
echo &quot;SNIPPET 5: copy_fingerprint.sh&quot;

export BUILD_ID=dontKillMe
~/epages-infra/scripts/epages/fingerprint/copy_fingerprint.sh -v &quot;${VERSION}&quot; -r &quot;${REPO}&quot; -i &apos;unity&apos; -t &apos;install&apos; -s /tmp/epages-installation-fingerprint</command>
    </hudson.tasks.Shell>
