        <url>git@github.com:ePages-de/multistore-MS.git</url>
        <credentialsId>f57a52ae-2c3d-4b26-9342-0eac1f047740</credentialsId>
      </hudson.plugins.git.UserRemoteConfig>
    </userRemoteConfigs>
