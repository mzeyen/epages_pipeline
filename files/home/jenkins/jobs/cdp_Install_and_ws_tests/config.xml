    <hudson.tasks.Shell>
      <command>#!/bin/bash

echo &quot;For testing: set selinux=0&quot;
echo 0 &gt;/selinux/enforce




# Deploy epages-infra repo.
if [[ ! -d ~/epages-infra/.git ]] ; then
    echo &apos;Deploying repo epages-infra...&apos;
    cd ~
    git clone git@github.com:ePages-de/epages-infra.git
else
    echo &apos;Updating master branch of repo epages-infra...&apos;
    cd ~/epages-infra &amp;&amp; git pull origin master
fi</command>
    </hudson.tasks.Shell>
    <hudson.tasks.Shell>
      <command>export BUILD_ID=dontKillMe
~/epages-infra/scripts/epages/jenkins-installs-epages.sh -i simple/install-epages-390MBx6.conf -r ftp://ftp.epages.com/pub/epages/build/repo/$REPO</command>
    </hudson.tasks.Shell>
    <hudson.tasks.Shell>
      <command>export BUILD_ID=dontKillMe
~/epages-infra/scripts/epages/test/run_web_service_tests.sh -d &quot;`hostname --fqdn`&quot;&apos;&apos; -b dev</command>
    </hudson.tasks.Shell>
